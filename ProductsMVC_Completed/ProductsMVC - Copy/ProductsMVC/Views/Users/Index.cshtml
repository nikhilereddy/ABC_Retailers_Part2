@model IEnumerable<ProductsMVC.Models.User>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.CustEmail)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CustPassword)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.CustEmail)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.CustPassword)
                </td>
                <td>
                    <a asp-action="Edit" asp-route-id="@item.RowKey">Edit</a> |
                    <a asp-action="Details" asp-route-id="@item.RowKey">Details</a> |
                    <a asp-action="Delete" asp-route-id="@item.RowKey">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>

